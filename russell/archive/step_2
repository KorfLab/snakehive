rule all:
    input:
        's2_shell.txt',
        's2_python.txt'

rule shell_write:
    output:
        's2_shell.txt'
    shell:
        'echo "hello from shell" > {output}'

rule python_write:
    output:
        's2_python.txt'
    run:
        with open(output[0], 'w') as out:
            out.write('hello world from python')