rule touch:
    output:
        'results/3.2_touch.txt'
    shell:
        'touch {output}'

rule grep:
    input:
        '1pct.fa'
    output:
        'results/3.2_grep.txt'
    shell:
        'grep -c ATG {input} > {output}'

rule echo:
    output:
        'results/3.2_echo.txt'
    shell:
        'echo {output} > {output}'