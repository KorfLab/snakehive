rule touch:
    output:
        'results/3.3_touch.txt'
    shell:
        'touch {output}'

rule grep:
    input:
        '1pct.fa'
    output:
        'results/3.3_grep.txt'
    shell:
        'grep -c ATG {input} > {output}'

rule echo:
    default_target: True
    output:
        'results/3.3_echo.txt'
    shell:
        'echo {output} > {output}'